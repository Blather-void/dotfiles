priority -1

snippet mksnip "A snippet for writing Ultisnips snippets" b
`!p snip.rv = "snippet"` ${1:trigger} "${2:Description}" ${3:options}
$4
`!p snip.rv = "endsnippet"`
$0
endsnippet

snippet regexmain "does the regex for alphabet " 
(^[^a-zA-Z]\\)
endsnippet


snippet "([ =\{\(\[])'" "Single quotation marks" rA
`!p snip.rv = match.group(1)`'${1:${VISUAL:}}'$0
endsnippet

snippet "([ =\{\(\[])\"" "Double quotation marks" rA
`!p snip.rv = match.group(1)`"${1:${VISUAL:}}"$0
endsnippet

